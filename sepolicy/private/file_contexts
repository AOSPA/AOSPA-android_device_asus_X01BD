# File System
/persist(/.*)?                   u:object_r:persist_file:s0

# Sysfs
/sys/devices(/platform)?/soc/[a-z0-9]+.qcom,spmi/spmi-[0-9]/spmi0-0[0-9]/[a-z0-9]+.qcom,spmi:qcom,[a-z0-9]+@[0-9]:qcom,qpnp-smb2/power_supply/usb(/.*)?		u:object_r:sysfs_usb_supply:s0
